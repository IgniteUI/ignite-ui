{
    "preset": "jquery",
    "requireSpacesInsideParentheses": false,
    "disallowSpacesInFunctionExpression": false,
    "disallowSpacesInAnonymousFunctionExpression": false,
    "validateLineBreaks": null,
    "requireSpacesInsideBrackets": {
        "allExcept": [ "[", "]", "{", "}" ]
    },
    "requireCapitalizedComments": false,
    "maximumLineLength": {
        "value": 100,
        "allExcept": [ "regex", "comments", "urlComments" ]
    },
    "requirePaddingNewLinesBeforeLineComments": { "allExcept": "firstAfterCurly" }
}
